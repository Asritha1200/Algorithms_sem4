//NON RECURSIVE
#include <stdio.h>
void binarysearch(int *a,int key,int n)
{
    int low=a[0];
    int high=a[n-1];
    int mid;
    while(low<=high)
    {
       mid =(low+high)/2;
        if(a[mid]==key)
        {
            printf("ele found at %d",mid);
            return;
        }
        else if(key<mid)
        {
            high=mid-1;
        }
        else
        {
            low=mid+1;
        }
    }
    printf("not found");
}



int main()
{
    int arr[10];int key;
    int n;
    printf("enter size of arr");
    scanf("%d",&n);
    printf("enter the sorted array ele");
    for(int i=0;i<n;i++)
    {
    scanf("%d",&arr[i]);
    }
    printf("enter the key");
    scanf("%d",&key);
    
    binarysearch(arr,key,n);
}
